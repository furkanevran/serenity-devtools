{
    "manifest_version": 3,
    "name": "Serenity DevTools",
    "version": "0.0.1",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.ts",
        "type": "module"
    },
    "devtools_page": "devtools.html",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script.ts"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [{
        "resources": ["devtools.png", "devtools.html", "devtools.js", "devtools.css"],
        "matches": ["<all_urls>"]
    }]
}