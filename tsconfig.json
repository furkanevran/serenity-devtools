{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "incremental": false,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "baseUrl": ".",
    "moduleResolution": "node",
    "target": "es2016",
    "module": "ESNext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
  }
}